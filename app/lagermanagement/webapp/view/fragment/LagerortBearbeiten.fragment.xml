<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form">
    <f:SimpleForm
        id="SimpleFormChange354"
        editable="true"
        layout="ResponsiveGridLayout"
        title="{i18n>title.LagerBearbeiten}"
        labelSpanXL="3"
        labelSpanL="3"
        labelSpanM="3"
        labelSpanS="12"
        adjustLabelSpan="false"
        emptySpanXL="4"
        emptySpanL="4"
        emptySpanM="4"
        emptySpanS="0"
        columnsXL="1"
        columnsL="1"
        columnsM="1"
        singleContainerFullSize="false"
    >
        <f:content>
            <Label text="{i18n>lagerort.LagerName}" labelFor="edit_input_lieferant" />
            <Input value="{name}" />
            <Label text="{i18n>lagerort.Lagerort}" labelFor="edit_input_lieferant" />
            <Input value="{ort}" />
        </f:content>
    </f:SimpleForm>
</core:FragmentDefinition>
